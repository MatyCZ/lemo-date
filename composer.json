{
  "name": "matycz/lemo-date",
  "description": "Set of date utilities for Laminas applications",
  "license": "BSD-3-Clause",
  "type": "library",
  "keywords": [
    "laminas",
    "date",
    "holidays"
  ],
  "homepage": "https://github.com/MatyCZ/lemo-date",
  "authors": [
    {
      "name": "Matej Szendi"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lemo\\Date\\": "src/"
    }
  },
  "require": {
    "ext-calendar": "*",
    "ext-intl": "*",
    "laminas/laminas-stdlib": "^3.0",
    "php": "^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~3.8.0",
    "phpstan/phpstan": "~1.7.0"
  },
  "scripts": {
    "static-analysis": "phpstan analyse -c phpstan.neon"
  }
}
